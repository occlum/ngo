From 214fa2550e0c1c76fea3933e530267f856f6849d Mon Sep 17 00:00:00 2001
From: Shuocheng Wang <wangshch5@outlook.com>
Date: Mon, 16 Aug 2021 17:06:02 +0800
Subject: [PATCH] Update rust toolchain

---
 .github/workflows/main.yml | 2 +-
 rust-toolchain             | 2 +-
 sefs-cli/rust-toolchain    | 2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/.github/workflows/main.yml b/.github/workflows/main.yml
index c955047..c1f47ab 100644
--- a/.github/workflows/main.yml
+++ b/.github/workflows/main.yml
@@ -11,7 +11,7 @@ jobs:
     - uses: actions-rs/toolchain@v1
       with:
         profile: minimal
-        toolchain: nightly-2020-04-07
+        toolchain: nightly-2020-10-25
         override: true
         components: rustfmt, clippy
     - name: Check code format
diff --git a/rust-toolchain b/rust-toolchain
index 4e74872..8c975dc 100644
--- a/rust-toolchain
+++ b/rust-toolchain
@@ -1 +1 @@
-nightly-2020-04-07
+nightly-2020-10-25
diff --git a/sefs-cli/rust-toolchain b/sefs-cli/rust-toolchain
index 4e74872..8c975dc 100644
--- a/sefs-cli/rust-toolchain
+++ b/sefs-cli/rust-toolchain
@@ -1 +1 @@
-nightly-2020-04-07
+nightly-2020-10-25
